{
    "name": "grazia/woocommerce-excel-importer",
    "description": "Enterprise-level WooCommerce product catalog management via Excel import/export",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Grazia",
            "email": "info@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "phpoffice/phpspreadsheet": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "WooExcelImporter\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require-dev": {
        "php-stubs/wordpress-stubs": "^6.9"
    }
}
